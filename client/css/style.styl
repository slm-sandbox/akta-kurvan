@import "normalize.css"

*
  box-sizing: border-box;

#field
  height: 100%
  width: auto
  background-color: black


svg
  position: fixed
  top: 0
  left: 0
  bottom: 0
  right: 0
  width: 100%
  height: 100%

  background-color: black
  border: 1px solid yellow

  path
    stroke-width: 10px
    stroke-linecap: round

  .own
    stroke: green
  .enemy
    stroke: red
